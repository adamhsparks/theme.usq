year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{theme.usq}: USQ Themed Plots from R",
         author = c(person("Adam", "Sparks")),
         year = year,
         note = note,
         DOI = "10.5281/zenodo.1044768",
         url = "https://github.com/adamhsparks/theme.usq/")
